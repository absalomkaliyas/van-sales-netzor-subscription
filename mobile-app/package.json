{
  "name": "@van-sales/mobile-app",
  "version": "1.0.0",
  "main": "expo-router",
  "scripts": {
    "start": "expo start",
    "start:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/start-fixed.ps1",
    "fix:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/fix-expo-windows.ps1",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:now": "powershell -ExecutionPolicy Bypass -File ./scripts/build-now.ps1",
    "auto-fix": "powershell -ExecutionPolicy Bypass -File ./scripts/build-now.ps1",
    "build:local": "powershell -ExecutionPolicy Bypass -File ./scripts/build-local-android.ps1",
    "build:rn": "powershell -ExecutionPolicy Bypass -File ./scripts/build-with-react-native-cli.ps1"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.1.9",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "date-fns": "^3.0.6",
    "expo": "~50.0.0",
    "expo-build-properties": "~0.12.5",
    "expo-modules-core": "~1.11.0",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-file-system": "~16.0.9",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-router": "~3.5.24",
    "expo-sharing": "~12.0.1",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-pdf": "^6.7.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "~5.3.3"
  },
  "private": true
